algoritmo "pratica1"
var
	 n, i, t : inteiro
inicio
    repita
        i <- 0
        escreva("Digite o número da tabuada :")
        leia (n)
        enquanto (i <= 10) e (n >0) faca
          t <- i * n
          escreval (i, " x ", n, " = ", t)
          i<- i+1
     fimenquanto
   ate n = 0
fimalgoritmo

